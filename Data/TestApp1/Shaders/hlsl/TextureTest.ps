Texture2D inputTexture;

SamplerState S
{
	MipFilter = LINEAR;
    MinFilter = LINEAR;
    MagFilter = LINEAR;
};

void main(
	in float2 texCoords : TEXCOORD0,
	out float4 color : SV_Target
)
{
    color = inputTexture.Sample(S, texCoords);
}
